Global:
    cache_root: D:/Cache
    SF_raw: //161.55.120.117/NMML_AcousticsData/Audio_Data
    SF_foc: //161.55.120.117/NMML_AcousticsData/Audio_Data/DecimatedWaves
    Wrapper: False
    parameters:
        signal_codes: RB
        freq_start: 0
        freq_size: 4000
        native_height: 240
        npps: 120
        native_window_size: 240
        model_window_size: 240
        model_name: EffecientNet
        inf_batch_size: 175
        #msp_inf: 10
        msp_inf: 120
        split_prot: within_file
        querygt1:
            > "SELECT DISTINCT detections.* FROM detections JOIN bins_detections on bins_detections.detections_id = detections.id JOIN bins on bins.id = bins_detections.bins_id 
            > WHERE detections.status = 1 AND detections.signal_code = 17 AND label = 1 AND procedure = 10 AND bins.id IN {FG}"
Job:
    ModelEval_NN:
        parameters:
            methodID: simple-eval
            methodvers: v1-3
            gt_depth: [signal_codes]
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: Python
    DLmodel*:
        parameters:
            methodID: train-win-slide-dl
            methodvers: v1-20
            augstr: 0.20,0.55,1
            gt_depth: [signal_codes]
            model_name: [model_name]
            native_height: [native_height]
            native_pix_per_sec: [npps]
            only_labs: n
            stride_pix: [msp_inf]
            #stride_pix: 40
            win_size: [model_window_size]
            win_size_native: [native_window_size]
        arguments:
            batch_size: [inf_batch_size]
            hard_rerun: n
        descriptors:
            runtype: lib
            language: Python
            venv: Conda
            venv_name: tf-gpu4
    FormatFG:
        parameters:
            decimate_data: y
            difftime_limit: 3600
            methodID: pampgdb-standard-pullfg
            methodvers: v1-2
            methodID2m: matlabdecimate
            methodvers2m: V1s0
            target_samp_rate: 8192
            file_groupID:
                #- AL22_AU_PH01
                - AL21_AU_IC02
                - AL20_AU_BF02
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
            runtype2m: bin
            language2m: MATLAB
    TrainModel_paramset:
        FormatGT:
            parameters:
                methodID: pampgdb-standard-pullgt
                methodvers: v1-4
                query: [querygt1]
                #below is "n" or the int signal code to standardize to
                standardize_sig_code: n
                remove_proc_ovlp: n
            arguments:
                hard_rerun: n
            descriptors:
                runtype: lib
                language: R
        FormatFG:
            parameters:
                decimate_data: y
                difftime_limit: 3600
                methodID: pampgdb-standard-pullfg
                methodvers: v1-2
                methodID2m: matlabdecimate
                methodvers2m: V1s0
                target_samp_rate: 8192
                file_groupID:
                    - jasco_bearded_2009_2010
                    - jasco_bearded_2007_2008
            arguments:
                hard_rerun: n
            descriptors:
                runtype: lib
                language: R
                runtype2m: bin
                language2m: MATLAB
        seperate_ds_train:
            SplitTensor:
                parameters:
                    methodID: tens-simple-split
                    methodvers: v1-3
                    native_pix_per_sec: [npps]
                    seed_val: 1
                    split_protocol: [split_prot]
                    test_split: n
                    train_test_split: 1.0
                    train_val_split: 0.75
                    use_seed: y    
                arguments:
                    hard_rerun: n
                descriptors:
                    runtype: lib
                    language: R
        test1:
            DLmodel*:
                parameters:
                    methodID: train-win-slide-dl
                    methodvers: v1-20
                    addnoise_mean: 0.5
                    addnoise_stddev: 0.25
                    augstr: 0.20,0.55,1
                    do_aug: y
                    do_spec_averaging: y
                    do_val: y
                    do_weight: n
                    gt_depth: [signal_codes]
                    learn_rate: 0.00015
                    model_name: [model_name]
                    native_height: [native_height]
                    native_pix_per_sec: [npps]
                    perc_fp: 1.00
                    perc_tp: 0.025
                    #prop_fp: 0.9
                    #prop_tp: 0.1
                    prop_fp: 0.75
                    prop_tp: 0.25
                    shuf_size: 0.0005
                    stride_pix: 30
                    tp_weight: 2
                    view_plots: n
                    win_size: [model_window_size]
                    win_size_native: [native_window_size]
                    zero_drop: 0.1
                arguments:
                    batch_size: 75
                    batch_size_inf: [inf_batch_size]
                    #save these, others are for testing
                    epoch: 10
                    epoch_steps: 4500
                    a_val_steps: 3000
                    #epoch: 2
                    #epoch_steps: 30
                    #a_val_steps: 30
                    a_val_tp_heavy: y
                    hard_rerun: n
                descriptors:
                    runtype: lib
                    language: Python
                    wrapper: True
                    pass_sub_to_wrapper: True
                    venv: Conda
                    venv_name: tf-gpu4
    FormatGT:
        parameters:
            methodID: pampgdb-standard-pullgt
            methodvers: v1-4
            query: [querygt1]
            remove_proc_ovlp: n
            standardize_sig_code: n
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
    GenBigSpec:
        parameters:
            methodID: con_bright_no_rep
            methodvers: v1-5
            crop_freq: n
            crop_freq_size: [freq_size]
            crop_freq_start: [freq_start]
            native_height: [native_height]
            native_pix_per_sec: [npps]
            window_length: 512
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
    LabelTensor:
        parameters:
            methodID: tens-simple-label
            methodvers: v1-8
            dimensions: 2
            freq_size: [freq_size]
            freq_start: [freq_start]
            native_height: [native_height]
            native_pix_per_sec: [npps]
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
    SplitTensor:
        parameters:
            methodID: tens-simple-split
            methodvers: v1-3
            native_pix_per_sec: [npps]
            seed_val: 1
            split_protocol: within_file
            test_split: y
            train_test_split: 0.80
            train_val_split: 0.75
            use_seed: y    
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
    seperate_ds_test:
        SplitTensor:
            parameters:
                methodID: tens-simple-split
                methodvers: v1-3
                native_pix_per_sec: [npps]
                seed_val: 1
                split_protocol: within_file
                test_split: n
                train_test_split: 0
                train_val_split: 0.75
                use_seed: y    
            arguments:
                hard_rerun: n
            descriptors:
                runtype: lib
                language: R
    RavenViewDETx:
        parameters:
            methodID: rv-simple-w-metadata
            methodvers: v1-10
        arguments:
            fg_fill: y
            hard_rerun: n
            ignore_decimated: n
        descriptors:
            runtype: lib
            language: R
    RavenToDETx:
        parameters:
            methodID: rd-simple-w-metadata
            methodvers: v1-5
        descriptors:
            runtype: lib
            language: R
    AssignLabels:
        parameters:
            methodID: labels-w-iou-simple
            methodvers: v1-10
            iou_thresh: 0.001
            write_GT: y
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
    PerfEval2DL:
        parameters:
            methodID: pe2dl-simple
            methodvers: v1-4
            cexmod: 2
            suppress_test: n
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
    EditRAVENx:
        parameters:
            instructions:
                >
                > INSTRUCTIONS: 
                >
                > Open up a Raven Pro 1.5 window. Holding the control key, drag and drop the file into the Raven window.        
                > Review the outputs and populate label column. Allowable labels are y/m/n (yes, maybe, or no).  
                > Comments can be made and will be retained in the database. Adding, deleting, or modifying timestamps or frequency of
                > detections is forbidden by protocol. 
                >
                > To revise your review, do not run this task again if it has been published! This will result in duplicates in the database. 
                > Instead, use the EditGTpublish pipeline. 
                >
                > If you are rerunning this process, there may be a previously edited file you are free to override or retain
        arguments:
            hard_rerun: n
        descriptors:
            runtype: no_method
    CompareAndPublishDets:
        parameters:
            methodID: pgpamdb-default-compare-publish
            methodvers: v1-7
        arguments:
            hard_rerun: n
        descriptors:
            language: R
            runtype: lib
    PublishDetswFG:
        parameters: 
            methodID: pampgdb-standard-publish
            methodvers: v1-3
            bin_fill_code: SHI
            procedure_id: 27
            signal_code: [signal_codes]
            #Standard DL methods will always be loose bound detection. 
            strength: loose
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
    ScoresToDETx:
        parameters:
            methodID: moving-smooth
            methodvers: v1-13
            difftime_cap: 3600
            freq_low: [freq_start]
            freq_size: [freq_size]
            #group_pixels: 20 temporary testing!!
            group_pixels: 120
            mod_size_1: [native_window_size]
            mod_size_2: [model_window_size]
            native_pix_per_sec: [npps]
            smooth_method: mean
            split_protocol: [split_prot]
            stride_pix: [msp_inf]
        arguments:
            hard_rerun: n
            #make sure remove spec is not on when running model perf eval and want to hang onto spectrograms!
            remove_spec: y
        descriptors:
            runtype: lib
            language: R
    ApplyCutoff:
        parameters:
            cutoff: 0.85
        arguments:
            hard_rerun: n
        descriptors:
            runtype: no_method
    stats_calc:
        ApplyCutoff:
            parameters:
                append_cutoff: y
                cutoff:
                    - 0.0
                    #- 0.1
                    - 0.4
                    - 0.25
                    - 0.5
                    - 0.6
                    - 0.75
                    - 0.8
                    - 0.85
                    - 0.9
                    #- 0.95
                    #- 0.99
                    #- 0.999
            arguments:
                hard_rerun: n
            descriptors:
                runtype: no_method
    PerfEval1DL:
        parameters:
            methodID: pe1dl-simple
            methodvers: v1-2
            suppress_test: n
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
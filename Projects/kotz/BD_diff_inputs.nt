            methodvers: v1-4
            cexmod: 2
            suppress_test: n
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
    PublishDetswFG:
        parameters: 
            methodID: pampgdb-standard-publish
            methodvers: v1-3
            bin_fill_code: SHI
            procedure_id: 37
            signal_code: [signal_codes]
            #Standard DL methods will always be loose bound detection. 
            strength: loose
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
    ScoresToDETx:
        parameters:
            methodID: moving-smooth
            methodvers: v1-13
            difftime_cap: 3600
            freq_low: [freq_start]
            freq_size: [freq_size]
            #PROC 30 CHANGE: attempt to lower the total # of dets. 
            group_pixels: 120
            mod_size_1: [native_window_size]
            mod_size_2: [model_window_size]
            native_pix_per_sec: [npps]
            smooth_method: mean
            split_protocol: [split_prot]
            stride_pix: [msp_inf]
        arguments:
            hard_rerun: n
            #make sure remove spec is not on when running model perf eval and want to hang onto spectrograms!
            #CHANGE WITH  INFERENCE!
            remove_spec: y
        descriptors:
            runtype: lib
            language: R

Global:
    cache_root: ../cache
    SF_raw: C:/pamdata_mount/afsc-1/bottom_mounted
    SF_foc: T:/DecimatedWaves
    Wrapper: False
    parameters:
        signal_codes: FW
        freq_start: 0
        freq_size: 64
        native_height: 128
        npps: 20
        native_window_size: 80
        model_window_size: 240
        model_name: EffecientNet
        inf_batch_size: 175
        #msp_inf: 10
        msp_inf: 40
        split_prot: within_file
        querygt_low:
            > "SELECT DISTINCT detections.* FROM detections JOIN bins_detections on bins_detections.detections_id = detections.id JOIN bins on bins.id = bins_detections.bins_id 
            > WHERE detections.status = 1 AND detections.signal_code IN (4,5,6) AND low_freq < 25 AND label = 1 AND procedure = 10 AND bins.id IN {FG}"
Job:
    ModelEval_NN:
        parameters:
            methodID: simple-eval
            methodvers: v1-3
            gt_depth: [signal_codes]
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: Python
    DLmodel*:
        parameters:
            methodID: train-win-slide-dl
            methodvers: v1-21
            augstr: 0.20,0.55,1
            gt_depth: [signal_codes]
            model_name: [model_name]
            native_height: [native_height]
            native_pix_per_sec: [npps]
            only_labs: n
            stride_pix: [msp_inf]
            #stride_pix: 40
            win_size: [model_window_size]
            win_size_native: [native_window_size]
        arguments:
            batch_size: [inf_batch_size]
            hard_rerun: n
        descriptors:
            runtype: lib
            language: Python
            venv: Conda
            venv_name: tf-gpu4
    FormatFG:
        parameters:
            decimate_data: y
            difftime_limit: 3600
            methodID: pampgdb-standard-pullfg
            methodvers: v1-3
            methodID2m: matlabdecimate
            methodvers2m: V1s1
            target_samp_rate: 256
            file_groupID:
                - GA22_AU_BT01
                - GA22_AU_SE01
                - GA22_AU_SH01
                - GA22_AU_SU01
                - BS22_AU_PM04
                - BS22_AU_PM05
                - AL22_AU_BS11
        arguments:
            hard_rerun: n
            prestage_data: n
            parallel_matlab: 7
        descriptors:
            runtype: lib
            language: R
            runtype2m: bin
            language2m: MATLAB
    TrainModel_paramset:
        FormatGT:
            parameters:
                methodID: pampgdb-standard-pullgt
                methodvers: v1-4
                query: [querygt_low]
                #below is "n" or the int signal code to standardize to
                standardize_sig_code: 4
                remove_proc_ovlp: y
            arguments:
                hard_rerun: n
            descriptors:
                runtype: lib
                language: R
        FormatFG:
            parameters:
                decimate_data: y
                difftime_limit: 600
                methodID: pampgdb-standard-pullfg
                methodvers: v1-3
                methodID2m: matlabdecimate
                methodvers2m: V1s1
                target_samp_rate: 128
                file_groupID:
                    - fw_og_train
                    - fw2gen_hardneg_ds
                    - fw2gen_strat_pos_fewfin1
                    - fw2gen_strat_pos_somefin1
                    - fw2gen_strat_pos_manyfin1
                    - fw1difficult
                    - fw2gen_oddtp
            arguments:
                hard_rerun: n
                prestage_data: n
                parallel_matlab: 7
            descriptors:
                runtype: lib
                language: R
                runtype2m: bin
                language2m: MATLAB
        seperate_ds_train:
            SplitTensor:
                parameters:
                    methodID: tens-simple-split
                    methodvers: v1-3
                    native_pix_per_sec: [npps]
                    seed_val: 1
                    split_protocol: [split_prot]
                    test_split: n
                    train_test_split: 1.0
                    train_val_split: 0.75
                    use_seed: y    
                arguments:
                    hard_rerun: n
                descriptors:
                    runtype: lib
                    language: R
        test1:
            DLmodel*:
                parameters:
                    methodID: train-win-slide-dl
                    methodvers: v1-21
                    addnoise_mean: 0.50
                    addnoise_stddev: 0.40
                    augstr: 0.50,0.50,0.75
                    addnoise_mean: 0.5
                    addnoise_stddev: 0.25
                    augstr: 0.20,0.55,1
                    do_aug: y
                    do_spec_averaging: y
                    do_val: y
                    do_weight: n
                    gt_depth: [signal_codes]
                    learn_rate: 0.000075
                    model_name: [model_name]
                    native_height: [native_height]
                    native_pix_per_sec: [npps]
                    perc_fp: 1.000
                    perc_tp: 0.1
                    prop_fp: 0.70
                    prop_tp: 0.30
                    shuf_size: 0.01
                    stride_pix: 30
                    tp_weight: 2
                    view_plots: y
                    win_size: [model_window_size]
                    win_size_native: [native_window_size]
                    zero_drop: n
                arguments:
                    batch_size: 75
                    batch_size_inf: [inf_batch_size]
                    #save these, others are for testing
                    epoch: 8
                    epoch_steps: 4500
                    a_val_steps: 0
                    #epoch: 2
                    #epoch_steps: 30
                    #a_val_steps: 30
                    a_val_tp_heavy: y
                    hard_rerun: n
                descriptors:
                    runtype: lib
                    language: Python
                    wrapper: True
                    pass_sub_to_wrapper: True
                    venv: Conda
                    venv_name: tf-gpu4
    GenBigSpec:
        parameters:
            methodID: mel
            methodvers: v1-1
            native_height: [native_height]
            native_pix_per_sec: [npps]
            outlier_perc: 99.99
            view_mode: n
            window_length: 128
        arguments:
            hard_rerun: n
            #rerun_key: 2
        descriptors:
            runtype: lib
            language: Python
            venv: Conda
            venv_name: mel-spec
    GenBigSpec:
        parameters:
            methodID: con_bright_no_rep
            methodvers: v1-5
            crop_freq: y
            crop_freq_size: [freq_size]
            crop_freq_start: [freq_start]
            native_height: [native_height]
            native_pix_per_sec: [npps]
            window_length: 128
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
    LabelTensor:
        parameters:
            methodID: tens-simple-label
            methodvers: v1-9
            #CAUTION: 2 not currently working correctly with mel spectrograms. 
            #dimensions: 2
            dimensions: 1
            freq_size: [freq_size]
            freq_start: [freq_start]
            native_height: [native_height]
            native_pix_per_sec: [npps]
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
    SplitTensor:
        parameters:
            methodID: tens-simple-split
            methodvers: v1-3
            native_pix_per_sec: [npps]
            seed_val: 1
            split_protocol: within_file
            test_split: y
            train_test_split: 0.80
            train_val_split: 0.75
            use_seed: y    
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
    seperate_ds_test:
        SplitTensor:
            parameters:
                methodID: tens-simple-split
                methodvers: v1-3
                native_pix_per_sec: [npps]
                seed_val: 1
                split_protocol: within_file
                test_split: n
                train_test_split: 0
                train_val_split: 0.75
                use_seed: y    
            arguments:
                hard_rerun: n
            descriptors:
                runtype: lib
                language: R
    RavenViewDETx:
        parameters:
            methodID: rv-simple-w-metadata
            methodvers: v1-18
        arguments:
            fg_fill: y
            hard_rerun: n
            ignore_decimated: n
        descriptors:
            runtype: lib
            language: R
    RavenToDETx:
        parameters:
            methodID: rd-simple-w-metadata
            methodvers: v1-5
        descriptors:
            runtype: lib
            language: R
    AssignLabels:
        parameters:
            #methodID: labels-label-tensor
            #methodvers: v1-1
            methodID: labels-w-iou-simple
            methodvers: v1-10
            #PercTP: 0.1
            iou_thresh: 0.001
            write_GT: y
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
    PerfEval2DL:
        parameters:
            methodID: pe2dl-simple
            methodvers: v1-4
            cexmod: 2
            suppress_test: n
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
    EditRAVENx:
        parameters:
            instructions:
                >
                > INSTRUCTIONS: 
                >
                > Open up a Raven Pro 1.5 window. Holding the control key, drag and drop the file into the Raven window.        
                > Review the outputs and populate label column. Allowable labels are y/m/n (yes, maybe, or no).  
                > Comments can be made and will be retained in the database. Adding, deleting, or modifying timestamps or frequency of
                > detections is forbidden by protocol. 
                >
                > To revise your review, do not run this task again if it has been published! This will result in duplicates in the database. 
                > Instead, use the EditGTpublish pipeline. 
                >
                > If you are rerunning this process, there may be a previously edited file you are free to override or retain
        arguments:
            hard_rerun: n
        descriptors:
            runtype: no_method
    PublishDetswFG:
        parameters: 
            methodID: pampgdb-standard-publish
            methodvers: v1-3
            bin_fill_code: LOW
            procedure_id: 42
            signal_code: [signal_codes]
            #Standard DL methods will always be loose bound detection. 
            strength: loose
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
    ScoresToDETx:
        parameters:
            methodID: moving-smooth
            methodvers: v1-13
            difftime_cap: 600
            freq_low: [freq_start]
            freq_size: [freq_size]
            #group_pixels: 20 temporary testing!!
            group_pixels: 80
            mod_size_1: [native_window_size]
            mod_size_2: [model_window_size]
            native_pix_per_sec: [npps]
            smooth_method: mean
            split_protocol: [split_prot]
            stride_pix: [msp_inf]
        arguments:
            hard_rerun: n
            #make sure remove spec is not on when running model perf eval and want to hang onto spectrograms!
            remove_spec: n
        descriptors:
            runtype: lib
            language: R
    ApplyCutoff:
        parameters:
            #try high! fewer dets is nice. 
            cutoff: 0.80
        arguments:
            hard_rerun: n
        descriptors:
            runtype: no_method
    stats_calc:
        ApplyCutoff:
            parameters:
                append_cutoff: y
                cutoff:
                    - 0.0
                    #- 0.1
                    #- 0.4
                    #- 0.25
                    #- 0.5
                    - 0.6
                    - 0.75
                    - 0.8
                    - 0.85
                    - 0.9
                    - 0.95
                    - 0.99
                    #- 0.999
            arguments:
                hard_rerun: n
            descriptors:
                runtype: no_method
    PerfEval1DL:
        parameters:
            methodID: pe1dl-simple
            methodvers: v1-2
            suppress_test: n
        arguments:
            hard_rerun: n
        descriptors:
            runtype: lib
            language: R
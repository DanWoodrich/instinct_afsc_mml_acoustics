pipe: DLmodel_Train.pipe
#this one I made a custom pipe shape for to make it less convoluted. Lots of dependency tie ins. 
DLmodel_Train.pipe:
  process: DLmodel_Train
  GetFG: 
    pipe: FormatFG.pipe
    loop_on: FormatFG
    FormatFG.pipe:
      process: FormatFG
  GetSpec: 
    pipe: GenBigSpec.pipe
    GenBigSpec.pipe:
      process: GenBigSpec
      GetFG:
        pipe: FormatFG.pipe
        loop_on: FormatFG
        FormatFG.pipe:
          process: FormatFG
  GetLabels: 
    pipe: LabelTensor.pipe
    LabelTensor.pipe:
      process: LabelTensor
      GetFG:
        pipe: FormatFG.pipe
        loop_on: FormatFG
        FormatFG.pipe:
          process: FormatFG
      GetGT:
        pipe: FormatGT.pipe
        loop_on: FormatFG
        FormatGT.pipe:
          process: FormatGT
          GetFG:
            pipe: FormatFG.pipe
            FormatFG.pipe:
              process: FormatFG
      GetImg:
        pipe: GenBigSpec.pipe
        GenBigSpec.pipe:
          process: GenBigSpec
          GetFG:
            pipe: FormatFG.pipe
            loop_on: FormatFG
            FormatFG.pipe:
              process: FormatFG
  GetSplits: 
    pipe: SplitTensor.pipe
    SplitTensor.pipe:
      process: SplitTensor
      GetImg:
        pipe: GenBigSpec.pipe
        GenBigSpec.pipe:
          process: GenBigSpec
          GetFG:
            pipe: FormatFG.pipe
            loop_on: FormatFG
            FormatFG.pipe:
              process: FormatFG
      GetFG:
        pipe: FormatFG.pipe
        loop_on: FormatFG
        FormatFG.pipe:
          process: FormatFG
pipe: DLmodel_Train.pipe
#this one I made a custom pipe shape for to make it less convoluted. Lots of dependency tie ins. 
#switching from combining on initial FG to processing in groups and combining after. 
DLmodel_Train.pipe:
  process: DLmodel_Train
  GetFG: 
    pipe: FormatFG.pipe
    loop_on: FormatFG
    FormatFG.pipe:
      process: FormatFG
  GetSpec: 
    pipe: GenBigSpec.pipe
    loop_on: FormatFG
    GenBigSpec.pipe:
      process: GenBigSpec
      GetFG:
        process: FormatFG
        #pipe: FormatFG.pipe
        #loop_on: FormatFG
        #FormatFG.pipe:
        #  process: FormatFG
  GetLabels: 
    pipe: LabelTensor.pipe
    loop_on: FormatFG
    LabelTensor.pipe:
      process: LabelTensor
      GetFG:
        process: FormatFG
        #pipe: FormatFG.pipe
        #loop_on: FormatFG
        #FormatFG.pipe:
        #  process: FormatFG
      GetGT:
        pipe: FormatGT.pipe
        #loop_on: FormatFG
        FormatGT.pipe:
          process: FormatGT
          GetFG:
            process: FormatFG
            #pipe: FormatFG.pipe
            #FormatFG.pipe:
            #  process: FormatFG
      GetImg:
        pipe: GenBigSpec.pipe
        GenBigSpec.pipe:
          process: GenBigSpec
          GetFG:
            process: FormatFG
            #pipe: FormatFG.pipe
            #loop_on: FormatFG
            #FormatFG.pipe:
            #  process: FormatFG
  GetSplits: 
    pipe: SplitTensor.pipe
    loop_on: FormatFG
    SplitTensor.pipe:
      process: SplitTensor
      GetImg:
        pipe: GenBigSpec.pipe
        GenBigSpec.pipe:
          process: GenBigSpec
          GetFG:
            process: FormatFG
            #pipe: FormatFG.pipe
            #loop_on: FormatFG
            #FormatFG.pipe:
            #  process: FormatFG
      GetFG:
        process: FormatFG
        #pipe: FormatFG.pipe
        #loop_on: FormatFG
        #FormatFG.pipe:
        #  process: FormatFG
      process: CalcPeaks
      GetDETx:
        pipe: FormatGT.pipe
        #loop_on: FormatFG
        FormatGT.pipe:
          process: FormatGT
          GetFG: 
            process: FormatFG
  GetFG:
    #loop_on: FormatFG
    pipe: FormatFG.pipe
    FormatFG.pipe:
      process: FormatFG

pipe: ViewDETx
loop_on: FormatFG
params_drop: TrainModel_paramset
ViewDETx:
  process: RavenViewDETx
  GetDETx:
    pipe: AssignLabels.pipe
    AssignLabels.pipe:
      process: AssignLabels
      GetDETx:
        pipe: ApplyCutoff.pipe
        ApplyCutoff.pipe:
          process: ApplyCutoff
          GetDETx:
            pipe: SplitForPE.pipe
            SplitForPE.pipe:
              process: SplitForPE
              GetModel_w_probs:
                pipe: ScoresToDETx.pipe
                pipe_link: 
                  name: EvalScores_Train.nt
              GetFG:
                process: FormatFG
      GetGT:
        process: FormatGT
      GetFG:
        process: FormatFG
  GetFG:
    process: FormatFG